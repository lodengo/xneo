<fees>
	<整体工程>
		<fee>

			<feeName>单项工程工程费合计</feeName>
			<feeExpr>sum(ccf(单项工程,工程总造价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>

			<feeName>整体工程费用合计</feeName>
			<feeExpr>0</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>

			<feeName>工程总造价</feeName>
			<feeExpr>cf(单项工程工程费合计)+cf(整体工程费用合计)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
	</整体工程>
	<单项工程>
		<fee>
			<feeName>单位工程工程费合计</feeName>
			<feeExpr>sum(ccf(单位工程,工程总造价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>单项工程费用合计</feeName>
			<feeExpr>cf(措施项目清单费用)+cf(其他项目清单费用)+cf(规费)+cf(税金)</feeExpr>
			<feeResult>0</feeResult>
			<fee>
				<feeName>措施项目清单费用</feeName>
				<feeExpr>0</feeExpr>
				<feeResult>0</feeResult>
			</fee>
			<fee>
				<feeName>其他项目清单费用</feeName>
				<feeExpr>0</feeExpr>
				<feeResult>0</feeResult>
			</fee>
			<fee>
				<feeName>规费</feeName>
				<feeExpr>cf(工程排污费)+cf(社会保障费)+cf(住房公积金)+cf(河道管理费)</feeExpr>
				<feeResult>0</feeResult>
				<fee>
					<feeName>工程排污费</feeName>
					<feeExpr>cf(措施项目清单费用)*f(费率)</feeExpr>
					<费率>0.0348</费率>
					<feeResult>0</feeResult>
				</fee>
				<fee>
					<feeName>社会保障费</feeName>
					<feeExpr>200*f(费率)</feeExpr>
					<费率>0.0348</费率>
					<feeResult>0</feeResult>
				</fee>
				<fee>
					<feeName>住房公积金</feeName>
					<feeExpr>cf(措施项目清单费用)*f(费率)</feeExpr>
					<费率>0.0348</费率>
					<feeResult>0</feeResult>
				</fee>
				<fee>
					<feeName>河道管理费</feeName>
					<feeExpr>(cf(措施项目清单费用)+cf(其他项目清单费用)+cf(工程排污费)+cf(社会保障费)+cf(住房公积金))*f(费率)</feeExpr>
					<费率>0.0348</费率>
					<feeResult>0</feeResult>
				</fee>
			</fee>
			<fee>
				<feeName>税金</feeName>
				<feeExpr>(cf(措施项目清单费用)+cf(其他项目清单费用)+cf(规费)-cf(河道管理费))*f(费率)
				</feeExpr>
				<费率>0.0348</费率>
				<feeResult>0</feeResult>
			</fee>
		</fee>
		<fee>
			<feeName>工程总造价</feeName>
			<feeExpr>[单位工程工程费合计]+[单项工程费用合计]</feeExpr>
			<feeResult>0</feeResult>
		</fee>
	</单项工程>
	<单位工程>
		<fee>
			<feeName>分部分项工程量清单费用</feeName>
			<feeExpr>sum(ccf(分部分项,综合合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>措施项目清单费用</feeName>
			<feeExpr>1+2*3</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>其他项目清单费用</feeName>
			<feeExpr>0</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>规费</feeName>
			<feeExpr>0</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>税金</feeName>
			<feeExpr>0</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>工程总造价</feeName>
			<feeExpr>cf(分部分项工程量清单费用)+cf(措施项目清单费用)+cf(其他项目清单费用)+cf(规费)+cf(税金)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
	</单位工程>
	<分部分项>
		<fee>
			<feeName>人工费合价</feeName>
			<feeExpr>sum(ccf(清单,人工费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>材料费合价</feeName>
			<feeExpr>sum(ccf(清单,材料费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>机械费合价</feeName>
			<feeExpr>sum(ccf(清单,机械费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>直接费合价</feeName>
			<feeExpr>sum(ccf(清单,直接费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>综合合价</feeName>
			<feeExpr>sum(ccf(清单,综合合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
	</分部分项>
	<清单>
		<fee>
			<feeName>人工费</feeName>
			<feeExpr>cf(人工费合价)/c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>人工费合价</feeName>
			<feeExpr>sum(ccf(定额,人工费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>材料费</feeName>
			<feeExpr>cf(材料费合价)/c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>材料费合价</feeName>
			<feeExpr>sum(ccf(定额,材料费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>机械费</feeName>
			<feeExpr>cf(机械费合价)/c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>机械费合价</feeName>
			<feeExpr>sum(ccf(定额,机械费合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>直接费</feeName>
			<feeExpr>cf(人工费)+cf(材料费)+cf(机械费)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>直接费合价</feeName>
			<feeExpr>cf(直接费)*c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>综合单价</feeName>
			<feeExpr>cf(综合合价)/c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>综合合价</feeName>
			<feeExpr>sum(ccf(定额,综合合价))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
