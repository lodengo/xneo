		<fee>
			<feeName>人工费合价</feeName>
			<feeExpr>cf(人工费)*c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>材料费</feeName>
			<feeExpr>sum(ccf(材料,直接费))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>材料费合价</feeName>
			<feeExpr>cf(材料费)*c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>机械费</feeName>
			<feeExpr>sum(ccf(机械,直接费))</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>机械费合价</feeName>
			<feeExpr>cf(机械费)*c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>直接费</feeName>
