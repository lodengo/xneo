		<fee>
			<feeName>直接费合价</feeName>
			<feeExpr>cf(直接费)*c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>管理费</feeName>
			<feeExpr>0</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>管理费合价</feeName>
			<feeExpr>cf(管理费)*c(工程量)</feeExpr>
			<feeResult>0</feeResult>
		</fee>
		<fee>
			<feeName>利润</feeName>
			<feeExpr>0</feeExpr>
